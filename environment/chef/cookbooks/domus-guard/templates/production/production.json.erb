{
    "dbConfig": {
      "url"  : "mongodb://domus:domus1@127.0.0.1:27017/DomusGuard"
    },
	"onlyOneLocation" : true,
	"ssl": {
		"keyFile" : "/opt/domus/certs/domus.key",
		"pemFile" : "/opt/domus/certs/domus.pem"
	},
	"google": {
		"auth" : {
			"clientID"      : "<%= node['domusguard']['web']['google']['clientID']  %>",
			"clientSecret"  : "<%= node['domusguard']['web']['google']['clientSecret'] %>",
			"callbackURL"   : "<%= node['domusguard']['web']['google']['callbackURL'] %>"
		},
		"gcm" : "<%= node['domusguard']['web']['google']['gcm'] %>"
	}
}
